    "nuxt": "^3.0.0-rc.12",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.9",
    "@nuxt/content": "^2.2.0",
    "@nuxtjs/color-mode": "^3.1.8",
    "@unocss/nuxt": "^0.45.30",
    "@vueuse/nuxt": "^9.3.1",
    "nuxt-icon": "^0.1.7"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0-rc.12"
  }
